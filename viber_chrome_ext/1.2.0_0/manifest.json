{
   "background": {
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19@2x.png",
         "38": "images/icon-38@2x.png"
      },
      "default_title": "Viber"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "scripts/app.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_app_description__",
   "icons": {
      "128": "images/icon-128@2x.png",
      "16": "images/icon-16@2x.png",
      "19": "images/icon-19@2x.png",
      "38": "images/icon-38@2x.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Viber",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "storage", "http://*/*", "https://*/*", "contextMenus", "management" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0",
   "web_accessible_resources": [ "scripts/*", "images/*", "popup.html" ]
}
